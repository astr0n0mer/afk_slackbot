{
    "display_information": {
        "name": "AFKbot"
    },
    "features": {
        "bot_user": {
            "display_name": "AFKbot",
            "always_online": true
        },
        "slash_commands": [
            {
                "command": "/afk",
                "url": "https://eager-tips-fail.loca.lt/v1/slack_bot",
                "description": "Manage AFK status",
                "usage_hint": "for an hour",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "chat:write",
                "chat:write.customize",
                "chat:write.public",
                "commands",
                "users:read",
                "channels:join"
            ]
        }
    },
    "settings": {
        "interactivity": {
            "is_enabled": true,
            "request_url": "https://eager-tips-fail.loca.lt/v1/interactive_message"
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}
